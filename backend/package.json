    "jade": "~1.11.0",
    "jdenticon": "^3.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "nodemon": "^3.1.10"
  },
  "main": "app.js",
